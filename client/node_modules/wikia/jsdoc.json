{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include":["src"],
        "exclude": [],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "encoding": "utf8",
      "recurse": true,
      "private": false,
      "lenient": true,
    	"readme":"README.md",
    	"package":"package.json",
    	"destination":"./docs/",
    	"template":"./node_modules/@pixi/jsdoc-template"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "applicationName": "Wikia",
        "meta": {
          "description": "API Wrapper for the Wikia API",
          "title": "Wikia",
          "keyword": "wikia"
        }
    }
}
